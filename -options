-- --------------------------------------------------------

--
-- Estructura de tabla para la tabla `wp_bp_user_blogs_blogmeta`
--

CREATE TABLE IF NOT EXISTS `wp_bp_user_blogs_blogmeta` (
`id` bigint(20) NOT NULL,
  `blog_id` bigint(20) NOT NULL,
  `meta_key` varchar(255) DEFAULT NULL,
  `meta_value` longtext
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

--
-- Volcado de datos para la tabla `wp_bp_user_blogs_blogmeta`
--

INSERT INTO `wp_bp_user_blogs_blogmeta` (`id`, `blog_id`, `meta_key`, `meta_value`) VALUES
