`id` bigint(20) NOT NULL,
  `user_id` bigint(20) NOT NULL,
  `blog_id` bigint(20) NOT NULL
) ENGINE=InnoDB AUTO_INCREMENT=551 DEFAULT CHARSET=utf8;

