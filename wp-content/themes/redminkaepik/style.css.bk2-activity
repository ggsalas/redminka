	margin-left: 2.5rem;
}

.entry-content ol li {
	list-style-type: decimal;
}

.entry-content ul li {
	list-style-type: disc;
}

.entry-content ol ol,
.entry-content ul ul {
	margin-bottom: 0;
}


/*
14 Post Navigation
---------------------------------------------------------------------------------------------------- */

.navigation li {
	display: inline;
}

.navigation li a,
.navigation li a:hover,
.navigation li.active a,
.navigation li.disabled {
	background-color: #1e1e1e;
	cursor: pointer;
	font-size: 14px;
	font-weight: 400;
	padding: 4px 12px 6px;
	padding: 0.25rem 0.75rem 0.375rem;
}

.navigation li a:hover,
.navigation li.active a {
	background-color: #303236;
}


